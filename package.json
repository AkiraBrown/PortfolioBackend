{
  "name": "portfoliopagebackend",
  "version": "1.0.0",
  "description": "This is the backend for my Portfolio site.",
  "main": "server.js",
  "scripts": {
    "test": "jest",
    "dev": "nodemon server.js",
    "seedProj": "psql -U postgres -f ./db/ProjectSeed.sql",
    "seedBlog": "node ./src/db/BlogSeed.js",
    "seed": "psql -U postgres -f ./db/ProjectSeed.sql; node ./db/BlogSeed.js",
    "pg:init": "psql -U postgres -f ./db/schema.sql",
    "restart": "psql -U postgres -f ./db/schema.sql; psql -U postgres -f ./db/ProjectSeed.sql; node ./db/BlogSeed.js ;nodemon server.js",
    "prepare": "husky install"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^16.4.0",
    "express": "^4.19.2",
    "marked": "^13.0.3",
    "pg-promise": "^11.9.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.3.0",
    "@commitlint/config-conventional": "^19.2.2",
    "husky": "^8.0.3",
    "jest": "^29.7.0",
    "morgan": "^1.10.0",
    "nodemon": "^3.1.4",
    "supertest": "^7.0.0"
  }
}
